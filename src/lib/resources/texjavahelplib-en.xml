<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>English language file for texjavahelplib</comment>

<!-- Help Menu -->
<entry key="menu.help">Help</entry>
<entry key="menu.help.mnemonic">H</entry>
<entry key="menu.help.manual">Manual</entry>
<entry key="menu.help.manual.mnemonic">M</entry>
<entry key="menu.help.manual.keystroke">F1</entry>

<entry key="action.help">Help</entry>
<entry key="action.help.mnemonic">H</entry>
<entry key="action.help.description">Open manual</entry>
<entry key="action.help.keystroke">shift F1</entry>

<!-- Navigation -->
<entry key="menu.help.navigation">Navigation</entry>
<entry key="menu.help.navigation.mnemonic">N</entry>

<entry key="help.navigation.home">Home</entry>
<entry key="help.navigation.home.tooltip">Home Page</entry>
<entry key="help.navigation.home.description">Go to the first page</entry>
<entry key="help.navigation.home.mnemonic">H</entry>
<entry key="help.navigation.home.keystroke">alt HOME</entry>

<entry key="help.navigation.up">Up</entry>
<entry key="help.navigation.up.mnemonic">U</entry>
<entry key="help.navigation.up.tooltip">Up</entry>
<entry key="help.navigation.up.description">Go up a section</entry>
<entry key="help.navigation.up.keystroke">alt UP</entry>

<entry key="help.navigation.previous">Previous</entry>
<entry key="help.navigation.previous.mnemonic">P</entry>
<entry key="help.navigation.previous.tooltip">Previous Page</entry>
<entry key="help.navigation.previous.description">Go to the previous page</entry>
<entry key="help.navigation.previous.keystroke">alt LEFT</entry>

<entry key="help.navigation.next">Next</entry>
<entry key="help.navigation.next.mnemonic">N</entry>
<entry key="help.navigation.next.tooltip">Next Page</entry>
<entry key="help.navigation.next.description">Go to the next page</entry>
<entry key="help.navigation.next.keystroke">alt RIGHT</entry>

<entry key="help.navigation.search">Search...</entry>
<entry key="help.navigation.search.mnemonic">S</entry>
<entry key="help.navigation.search.tooltip">Search document</entry>
<entry key="help.navigation.search.description">Open search dialogue</entry>

<entry key="help.navigation.search.title">Search</entry>
<entry key="help.navigation.search.keywords">Keywords:</entry>
<entry key="help.navigation.search.keywords.mnemonic">K</entry>
<entry key="help.navigation.search.keywords.description">Search terms</entry>
<entry key="help.navigation.search.find">Find</entry>
<entry key="help.navigation.search.find.mnemonic">F</entry>
<entry key="help.navigation.search.find.tooltip">Find keywords in document</entry>
<entry key="help.navigation.search.case">Case-sensitive</entry>
<entry key="help.navigation.search.case.mnemonic">V</entry>
<entry key="help.navigation.search.exact">Match whole word</entry>
<entry key="help.navigation.search.exact.mnemonic">W</entry>

<entry key="help.navigation.searching">Searching...</entry>
<entry key="help.navigation.search.stop">Stop</entry>
<entry key="help.navigation.search.stop.keystroke">shift ESC</entry>
<entry key="help.navigation.search.found">Found {0} {0,choice,1#match|1&lt;matches}</entry>
<entry key="help.navigation.search.not_found">Not found</entry>
<entry key="help.navigation.search_link_title">Go to start of section</entry>
<entry key="help.navigation.search_link_para">Go to paragraph</entry>
<entry key="help.navigation.search.clear_results">Clear</entry>
<entry key="help.navigation.search.clear_results.mnemnonic">C</entry>
<entry key="help.navigation.search.clear_results.tooltip">Clear results and keywords</entry>

<entry key="help.navigation.history">History...</entry>
<entry key="help.navigation.history.mnemonic">H</entry>
<entry key="help.navigation.history.keystroke">ctrl shift H</entry>
<entry key="help.navigation.history.tooltip">View History</entry>
<entry key="help.navigation.history.description">View recent page list</entry>
<entry key="help.navigation.history.title">Page History</entry>
<entry key="help.navigation.history.pointer">&#x261E;</entry>
<entry key="help.navigation.history.header">Recent List</entry>

<entry key="help.navigation.history.go">Go</entry>
<entry key="help.navigation.history.go.mnemonic">G</entry>
<entry key="help.navigation.history.go.tooltip">Go to selected page</entry>

<entry key="help.navigation.historyback">Back</entry>
<entry key="help.navigation.historyback.mnemonic">B</entry>
<entry key="help.navigation.historyback.keystroke">alt shift LEFT</entry>
<entry key="help.navigation.historyback.tooltip">History Back</entry>
<entry key="help.navigation.historyback.description">Go back</entry>

<entry key="help.navigation.historyforward">Forward</entry>
<entry key="help.navigation.historyforward.mnemonic">F</entry>
<entry key="help.navigation.historyforward.keystroke">alt shift RIGHT</entry>
<entry key="help.navigation.historyforward.tooltip">History Forward</entry>
<entry key="help.navigation.historyforward.description">Go forward</entry>

<!-- Navigation -->
<entry key="menu.help.settings">Settings</entry>
<entry key="menu.help.settings.mnemonic">S</entry>

<entry key="help.settings.font.decrease">Font Decrease</entry>
<entry key="help.settings.font.decrease.tooltip">Decrease Font Size</entry>
<entry key="help.settings.font.decrease.mnemonic">D</entry>
<entry key="help.settings.font.decrease.keystroke">LESS</entry>

<entry key="help.settings.font.increase">Font Increase</entry>
<entry key="help.settings.font.increase.tooltip">Increase Font Size</entry>
<entry key="help.settings.font.increase.mnemonic">I</entry>
<entry key="help.settings.font.increase.keystroke">GREATER</entry>

<entry key="help.settings.font">Font...</entry>
<entry key="help.settings.font.tooltip">Select Font</entry>
<entry key="help.settings.font.mnemonic">F</entry>
<entry key="help.settings.font.keystroke">ctrl F</entry>

<entry key="help.settings.font_sample">“Waltz, bad nymph! For quick jigs vex sylph’s fiddle. D’ye ken?”</entry>

<entry key="help.settings.font.title">Font</entry>
<entry key="help.settings.font.family">Font Family:</entry>
<entry key="help.settings.font.family.mnemonic">F</entry>
<entry key="help.settings.font.size">Font Size:</entry>
<entry key="help.settings.font.size.mnemonic">Z</entry>

<entry key="help.settings.nav">Lower Navigation...</entry>
<entry key="help.settings.nav.mnemonic">L</entry>

<entry key="help.settings.nav.title">Lower Navigation Settings</entry>
<entry key="help.settings.nav.label_limit">Maximum number of characters:</entry>
<entry key="help.settings.nav.label_limit.mnemonic">M</entry>
<entry key="help.settings.nav.show_label">Include text</entry>
<entry key="help.settings.nav.show_label.mnemonic">T</entry>

<!-- Common Actions -->
<entry key="action.okay">Okay</entry>
<entry key="action.okay.mnemonic">O</entry>
<entry key="action.okay.keystroke">shift ENTER</entry>

<entry key="action.cancel">Cancel</entry>
<entry key="action.cancel.mnemonic">C</entry>
<entry key="action.cancel.keystroke">ESCAPE</entry>

<entry key="action.apply">Apply</entry>
<entry key="action.apply.mnemonic">A</entry>

<entry key="action.close">Close</entry>
<entry key="action.close.mnemonic">C</entry>
<entry key="action.close.keystroke">ESCAPE</entry>

<!-- Index Map -->
<entry key="indexmap.name_description">{0}: {1}</entry>
<entry key="indexmap.short_description">{0}: {1}</entry>
<entry key="indexmap.long_short_description">{0} ({1}): {2}</entry>

<!--  Errors -->
<entry key="error.xml.more_than_one_tag">More than 1 &lt;{0}&gt; found</entry>
<entry key="error.xml.missing_attr_in_tag">Missing ''{0}'' attribute in &lt;{1}&gt;</entry>
<entry key="error.xml.int_attr_required_in_tag">Integer ''{0}'' attribute required in &lt;{1}&gt; (found ''{2}'')</entry>
<entry key="error.xml.tag_found_outside">&lt;{0}&gt; found outside &lt;{1}&gt;</entry>
<entry key="error.xml.tag_found_inside">&lt;{0}&gt; found inside &lt;{1}&gt;</entry>
<entry key="error.xml.unknown_tag">Unknown tag &lt;{0}&gt;</entry>
<entry key="error.xml.missing_tag">Missing tag &lt;{0}&gt;</entry>
<entry key="error.xml.missing_tag_for_node">Missing &lt;{0}&gt; for node ''{1}''</entry>
<entry key="error.xml.unknown_end_tag">Unknown end tag &lt;/{0}&gt; found</entry>
<entry key="error.xml.unexpected_chars">Unexpected content ''{0}'' found</entry>
<entry key="error.xml.unknown_context">No context found for ID {0} referenced by entry ''{1}''</entry>

<entry key="error.resource_not_found">Resource file ''{0}'' not found</entry>
<entry key="error.resource_not_found_for_node">Resource file ''{0}'' not found for node ''{1}''</entry>
<entry key="error.no_helpset">Helpset has not been initialised</entry>
<entry key="error.node_id_not_found">Node with ID ''{0}'' not found</entry>
<entry key="error.no_desktop">Desktop not supported on the current platform.
Unable to open URL {0}</entry>
<entry key="error.browse_failed">Unable to open browser to view {0}</entry>
<entry key="error.missing_search_term">Missing search term</entry>

</properties>
