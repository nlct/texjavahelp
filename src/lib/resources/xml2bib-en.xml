<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>English language file for xml2bib</comment>

<!--  Messages -->
<entry key="message.copying">Copying {0} -> {1}</entry>
<entry key="message.charset">Encoding: {0}</entry>

<!--  Syntax -->
<entry key="syntax.title">Syntax:</entry>
<entry key="syntax.options">{0} [&lt;option&gt;]+ &lt;XML file&gt;...</entry>
<entry key="syntax.in">{0} (or {1}) &lt;XML file&gt;		add &lt;XML file&gt; to the list of input files (cumulative).</entry>
<entry key="syntax.out">{0} (or {1}) &lt;BIB file&gt;	The output BIB file.</entry>
<entry key="syntax.provide-xml">{0} (or {1})	Copy XML source files from {2}'s resource directory if missing.</entry>
<entry key="syntax.copy-overwrite-xml">{0}	Copy XML source files from {2}'s resource directory if missing or if {2}'s file is newer.</entry>
<entry key="syntax.version">{0} (or {1})	Print version and exit.</entry>
<entry key="syntax.help">{0} (or {1})		Print this help message and exit.</entry>
<entry key="syntax.debug">{0}			Switch on debug mode.</entry>
<entry key="syntax.nodebug">{0}		Switch off debug mode. (Default.)</entry>
<entry key="syntax.out.charset">{0} &lt;charset&gt;		Set the character encoding to &lt;charset&gt; for the output file.</entry>
<entry key="syntax.bugreport">Report bugs to: {0}</entry>
<entry key="syntax.homepage">{0} home page: {1}</entry>

<!-- Syntax error messages -->
<entry key="error.syntax">Syntax error: {0}</entry>
<entry key="error.syntax.unknown_option">Unknown option ''{0}''. (Use --help or -h for help.)</entry>
<entry key="error.syntax.only_one">Only one {0} permitted</entry>
<entry key="error.syntax.missing_filename">Filename expected after {0}</entry>
<entry key="error.syntax.missing_input">File name expected after {0}</entry>
<entry key="error.syntax.missing_in">Missing input file</entry>
<entry key="error.syntax.missing_out">Missing output file</entry>
<entry key="error.syntax.missing_value">Value expected after {0}</entry>


</properties>
